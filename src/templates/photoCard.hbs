{{#each this}}

  <a class="photo-link" href="{{largeImageURL}}">
  <div class="photo-card"></div>
  <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" width="420" height="280" />
  
  <div class="info">
    <p class="info-item">
      <b>Likes</b>
      {{likes}}
    </p>
    <p class="info-item">
      <b>Views</b>
      {{views}}
    </p>
    <p class="info-item">
      <b>Comments</b>
      {{comments}}
    </p>
    <p class="info-item">
      <b>Downloads</b>
      {{downloads}}
    </p>
  </div>
</div>
</a>
{{/each}}